{
    "name": "make-it-app/laravel-favicon-extractor",
    "description": "A favicon extractor for Laravel",
    "keywords": [
        "laravel",
        "favicon"
    ],
    "homepage": "https://github.com/make-it-app/laravel-favicon-extractor",
    "license": "MIT",
    "authors": [
        {
            "name": "Stefan Bauer",
            "email": "mail@stefanbauer.me",
            "homepage": "https://stefanbauer.me"
        },
        {
            "name": "Stanislav Kabin",
            "email": "ioffline@make-it.app"
        }
    ],
    "require": {
        "php" : "^7.0|^8.0|^8.1",
        "illuminate/support": "^8.0|^9.0",
        "graham-campbell/guzzle-factory": "^3.0|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2|^7.0|^8.0|^9.0",
        "orchestra/testbench": "~3.5.0|~3.6.0",
        "mockery/mockery": "^1.1",
        "friendsofphp/php-cs-fixer": "^2.12"
    },
    "autoload": {
        "psr-4": {
            "MakeIT\\LaravelFaviconExtractor\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MakeIT\\LaravelFaviconExtractor\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MakeIT\\LaravelFaviconExtractor\\FaviconExtractorServiceProvider"
            ],
            "aliases": {
                "FaviconExtractor": "MakeIT\\LaravelFaviconExtractor\\Facades\\FaviconExtractor"
            }
        }
    }
}
